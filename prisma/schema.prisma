generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl", "debian-openssl-1.1.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DIRECT_URL")
}


model AppRunInfo {
  @@map("APP_RUN_INFO")

  appId         String   @id
  appName       String
  top           String?
  left           String?
  width         String?
  height        String?
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @default(now()) @map("updated_at")
}